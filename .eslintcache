          />\n          <Button type=\"submit\">Email Password</Button>\n        </form>\n      </div>\n    </AuthWrapper>\n  );\n};\n\nexport default withRouter(EmailPassword);\n","/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/redux/createStore.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/redux/rootReducer.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/redux/User/user.reducer.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/redux/User/user.types.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/redux/User/user.actions.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/pages/Dashboard/index.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/hoc/withAuth.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/customHooks/index.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/customHooks/useAuth.js",["132"],"import { useEffect } from \"react\";\nimport { useSelector } from \"react-redux\";\n\nconst mapState = ({ user }) => ({\n  currentUser: user.currentUser,\n});\n\nconst useAuth = (props) => {\n  const { currentUser } = useSelector(mapState);\n\n  useSelector(() => {\n    if (!currentUser) {\n        props.history.push(\"/login\")\n    }\n  }, [currentUser]);\n\n  return currentUser;\n};\n\nexport default useAuth;\n",{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","replacedBy":"136"},{"ruleId":"137","severity":1,"message":"138","line":3,"column":25,"nodeType":"139","messageId":"140","endLine":3,"endColumn":33},{"ruleId":"137","severity":1,"message":"141","line":21,"column":27,"nodeType":"139","messageId":"140","endLine":21,"endColumn":38},{"ruleId":"142","severity":1,"message":"143","line":42,"column":6,"nodeType":"144","endLine":42,"endColumn":8,"suggestions":"145"},{"ruleId":"146","severity":1,"message":"147","line":11,"column":11,"nodeType":"148","endLine":11,"endColumn":14},{"ruleId"