rm onSubmit={handleSubmit}>\n          <FormInput\n            type=\"email\"\n            name=\"email\"\n            value={email}\n            placeholder=\"Email\"\n            handleChange={(e) => {\n              setEmail(e.target.value);\n            }}\n          />\n          <Button type=\"submit\">Email Password</Button>\n        </form>\n      </div>\n    </AuthWrapper>\n  );\n};\n\nexport default EmailPassword;\n","/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/redux/createStore.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/redux/rootReducer.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/redux/User/user.reducer.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/redux/User/user.types.js",["278"],"const userTypes = {\nEMAIL_SIGN_IN_START:'EMAIL_SIGN_IN_START',\nSIGN_IN_SUCCESS: 'SIGN_IN_SUCCESS',\nCHECK_USER_SESSION: 'CHECK_USER_SESSION',\nSIGN_OUT_USER_START:'SIGN_OUT_USER_START',\nSIGN_OUT_USER_SUCCESS:'SIGN_OUT_USER_SUCCESS',\nSIGN_UP_USER_START:'SIGN_UP_USER_START',\nRESET_PASSWORD_START:'RESET_PASSWORD_START',\nRESET_PASSWORD_SUCCESS : 'RESET_PASSWORD_SUCCESS',\nGOOGLE_SIGN_IN_START:'GOOGLE_SIGN_IN_START',\nUSER_ERROR:'USER_ERROR',\nRESET_PASSWORD_SUCCESS : 'RESET_PASSWORD_SUCCESS', \n}\n\nexport default userTypes;","/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/redux/User/user.actions.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/pages/Dashboard/index.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/hoc/withAuth.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/customHooks/index.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/customHooks/useAuth.js",["279"],"import { useEffect } from \"react\";\nimport { useSelector } from \"react-redux\";\nimport {useHistory} from 'react-router-dom'\n\nconst mapState = ({ user }) => ({\n  currentUser: user.currentUser,\n});\n\nconst u