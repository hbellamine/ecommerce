n / create new user in firestore using google log in\nexport const handleUserProfile = async (userAuth, additionalData) => {\n  if (!userAuth) return;\n  //destructuring the uid from userAuth\n  const { uid } = userAuth;\n  const userRef = firestore.doc(`users/${uid}`);\n\n  const snapshot = await userRef.get();\n//if user doesn't exist then we create it\n  if (!snapshot.exists) {\n    const { displayName, email } = userAuth;\n    const timestamp = new Date();\n    try {\n      await userRef.set({\n        displayName,\n        email,\n        createdDate: timestamp,\n        ...additionalData,\n      });\n    } catch (err) {\n      console.log(err)\n    }\n  }\n\n  //we return userRef to update our local components with user informations\n  return userRef;\n};\n","/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/firebase/config.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/components/SignUp/index.js",["86"],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/components/Forms/FormInput/index.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/pages/Recovery/index.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/components/AuthWrapper/index.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/components/EmailPassword/index.js",[],{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","severity":1,"message":"92","line":11,"column":11,"nodeType":"93","endLine":11,"endColumn":14},{"ruleId":"91","severity":1,"message":"92","line":14,"column":11,"nodeType":"93","endLine":14,"endColumn":14},{"ruleId":"94","severity":1,"message":"95","line":5,"column":10,"nodeType":"96","messageId":"97","endLine":5,"endColumn":16},{"ruleId":"94","severity":1,"message":"98","line":32,"column":7,"nodeType":"96","messageId":"97","endLine":32,"endColumn":13},"no-native-reassign",["99"],"no-negated-in-lhs",["100"],"jsx-a11y/anchor-is-valid","The href attribute is re