./pages/Dashboard\";\n\nconst App = (props) => {\n const dispatch = useDispatch();\n\n  // useEffect(() => {\n\n\n  //   return () => {\n  //     // componentwillunmount to unsubscribe\n  //     authListener();\n  //   };\n  // }, []);\n\n  return (\n    <div className=\"App\">\n      {/* //switch will render the first route that matches */}\n      <Switch>\n        <Route\n          exact\n          path=\"/\"\n          render={() => (\n            <HomepageLayout>\n              <Homepage />\n            </HomepageLayout>\n          )}\n        />\n        <Route\n          path=\"/registration\"\n          render={() => (\n            <MainLayout>\n              <Registration />\n            </MainLayout>\n          )}\n        />\n        <Route\n          path=\"/login\"\n          render={() => (\n            <MainLayout>\n              <Login />\n            </MainLayout>\n          )}\n        />\n\n        <Route\n          path=\"/recovery\"\n          render={() => (\n            <MainLayout>\n              <Recovery />\n            </MainLayout>\n          )}\n        />\n\n        <Route\n          path=\"/dashboard\"\n          render={() => (\n            <WithAuth>\n              <MainLayout>\n                <Dashboard />\n              </MainLayout>\n            </WithAuth>\n          )}\n        />\n      </Switch>\n    </div>\n  );\n};\n\n\nexport default App;\n","/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/components/Header/index.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/pages/Homepage/index.js",[],"/Users/hamzabellamine/code/hbellamine/ecomreact/ecomreact/src/components/Directory/index.js",["138","139"],"import React from \"react\";\nimport Men from \"./../../assets/men.jpeg\";\nimport Woman from \"./../../assets/woman.jpeg\";\nimport \"./styles.scss\";\n\nconst Directory = (props) => {\n  return (\n    <div className=\"directory\">\n      <div className=\"wrap\">\n        <div className=\"item\" s